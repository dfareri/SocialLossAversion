	Set LASelfMixed = New Procedure
	LASelfMixed.Name = ebUCase_L & ebUCase_A & ebUCase_S & ebLCase_e & ebLCase_l & ebLCase_f & ebUCase_M & ebLCase_i & ebLCase_x & ebLCase_e & ebLCase_d
	'{B7FB17F3-0017-4E5D-AF28-3691D17EC646}
